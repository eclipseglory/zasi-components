
.flex-row {
  display: flex;
  justify-content: center;
  align-items: center
}

.shadow {
  box-shadow: 2px 2px 3px darkgray;
}

.full-width {
  width: 100%;
}

.full-height {
  height: 100%;
}

.full-fill {
  width: 100%;
  height: 100%
}

.round {
  border-radius: 50%;
}

.cell-row{
  width:100%;
  display:flex;
  box-sizing: border-box;
  align-items: center;
  justify-content: space-between;
  border-bottom:solid 1px whitesmoke;
}